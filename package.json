{
  "name": "openra-mi",
  "productName": "OpenRA-MI",
  "version": "1.0.0",
  "description": "A desktop application to make mod installation quicker for OpenRA project",
  "author": "Michel Kansou",
  "license": "ISC",
  "main": "main.js",
  "scripts": {
    "compass": "compass compile --force -e production -s compressed",
    "compass-watch-dev": "compass watch --force",
    "webpack-prod": "cross-env webpack --config webpack.config.js --progress ",
    "webpack-watch-dev": "cross-env webpack --config webpack.config.dev.js --progress -w -d --hot --inline",
    "start-prod": "NODE_ENV=production npm run webpack-prod && npm run compass && electron main.js",
    "start-dev": "NODE_ENV=development electron main.js & npm run compass-watch-dev & npm run webpack-watch-dev",
    "start": "NODE_ENV=development  && electron .",
    "watch": "npm run compass-watch-dev & electron main.js && npm run webpack-prod",
    "build-mac": "electron-packager ./ --platform=darwin --arch=all --out=dist/ --overwrite --icon=app/img/app-icon.icns --ignore=node_modules --app-version=1.0.0"
  },
  "bugs": {},
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-core": "^6.6.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "cross-env": "^1.0.7",
    "electron-builder": "^3.4.0",
    "electron-packager": "^6.0.2",
    "electron-prebuilt": "^0.37.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsx-loader": "^0.13.2",
    "mocha": "^2.4.5",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.9",
    "webpack": "^1.12.14",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "classnames": "^2.2.3",
    "immutable": "^3.7.6",
    "jquery": "^2.2.2",
    "json-loader": "^0.5.4",
    "jsonfile": "^2.2.3",
    "jstransform": "^11.0.3",
    "react": "^0.14.7",
    "react-addons-update": "^0.14.7",
    "react-deep-force-update": "^2.0.1",
    "react-dom": "^0.14.7",
    "react-dropzone": "^3.3.2",
    "react-file-input": "^0.2.5",
    "react-hot-loader": "^1.3.0",
    "react-mixin": "^3.0.3",
    "react-pager": "^1.1.2",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.0",
    "redux": "^3.3.1",
    "semantic-ui-popup": "^2.1.7",
    "semantic-ui-transition": "^2.1.7"
  }
}
